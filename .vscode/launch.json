  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Recorder",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/recorder/index.js",
      "env": {
        "PROTOCOL": "ws",
        "HOST": "localhost",
        "PORT": "18080",
        "WEBSOCKET_PATH": "/kabusapi/websocket",
        "POSTGRES_HOST": "localhost",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "postgres",
        "POSTGRES_PORT": "5432",
      }
    }
  ]
}